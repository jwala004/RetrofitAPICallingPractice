<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jwala.retrofitgetmethodexamplemediumjava">
    <!-- Internet Permission -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.DeleteSuccessfulActivity"></activity>
        <activity android:name=".activity.DeletePostActivity" />
        <activity android:name=".activity.ReceivePostResponseActivity" />
        <activity android:name=".activity.PostRequestActivity" />
        <activity android:name=".activity.GETRequestActivity" />
        <activity android:name=".activity.URLManipulationActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>